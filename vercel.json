{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/products",
      "dest": "src/server.js"
    },
    {
      "src": "/test",
      "dest": "/__test__/indexxx.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}